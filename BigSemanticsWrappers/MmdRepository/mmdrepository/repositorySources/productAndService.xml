    </scalar>

    <scalar name="price">
      <xpath>//ul[@class='purchase-info']/li[@class='total']//span[@itemprop='price']/text()</xpath>
    </scalar>

    <collection name="main_images">
      <xpath>//img[@class='hero-img hero-img ir']</xpath>
      <scalar name="location">
        <xpath>./@src</xpath>
      </scalar>
