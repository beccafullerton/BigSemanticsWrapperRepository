<meta_metadata_repository name="research_gate" package="ecologylab.bigsemantics.generated.library.researchGate" default_user_agent_name="chrome_2">
    <meta_metadata name="research_gate_article" parser="xpath" extends="scholarly_article">
        <selector url_regex="http://www.researchgate.net/publicatio.*" domain="researchgate.net"/>
        <example_url url="http://www.researchgate.net/publication/228563675_A_formula_for_the_volume_of_a_hyperbolic_tetrahedon"/>
        <scalar name="title">
            <xpath>//h1[@class='pub-title']</xpath>
        </scalar>
        <scalar name="description">
            <xpath>//div[@class='pub-abstract']/div/div/p</xpath>
        </scalar>
        <collection name="cited_in" child_type="research_gate_article">
            <xpath>//div[@class='pub-citations-list']/ul/li//h5</xpath>
            <scalar name="title">
                <xpath>./a/span</xpath>
            </scalar>
            <scalar name="location">
                <xpath>./a/@href</xpath>
                <regex_op regex="^" replace="/"></regex_op>
                
            </scalar>
        </collection>
        <collection name="similar_publications" child_type="research_gate_article">
            <xpath>//div[@class='pub-similar-publications pub-similar-publications-sidebar']//li//h5/a</xpath>
            <scalar name="title">
                <xpath>./span</xpath>
            </scalar>
            <scalar name="location">
                <xpath>./@href</xpath>
                <regex_op regex="^" replace="/"></regex_op>
            </scalar>
        </collection>
        <collection name="authors" child_type="research_gate_author">
            <xpath>//div[@class='publication-detail-author-list']/div/div</xpath>
            <scalar name="title">
                <xpath>.//div[@class='item-name']/a</xpath>
            </scalar>
            <scalar name="location">
                <xpath>.//div[@class='item-name']/a/@href</xpath>
                <regex_op regex="^" replace="/"></regex_op>
            </scalar>
        </collection>
    
    </meta_metadata>
    <meta_metadata name="research_gate_author" parser="xpath" extends="author">
        <selector url_regex="http://www.researchgate.net/profil.*" domain="researchgate.net"/>
       
        <scalar name="title">
            <xpath>//h1[@class='profile-header-name']/a</xpath>
        </scalar>
        <composite name="insitution" type="research_gate_institution">
            <xpath>//div[@class='truncate-single-line institution-name']//a</xpath>
            <scalar name="title">
                <xpath>.</xpath>
            </scalar>
            <scalar name="location">
                <xpath>./@href</xpath>
                <regex_op regex="^" replace="/"></regex_op>
            </scalar>
        </composite>
        <composite name="department" type="research_gate_institution_department">
            <xpath>//div[@class='truncate-single-line institution-dept']//a</xpath>
            <scalar name="title">
                <xpath>.</xpath>
            </scalar>
            <scalar name="location">
                <xpath>./@href</xpath>
                <regex_op regex="^" replace="/"></regex_op>
            </scalar>
        </composite>
        <composite name="stats" type="research_gate_stats">
            <xpath>//ul[@class='stats-list']</xpath>
            <scalar name="publication_count">
                <xpath>./li[1]/a/div[1]</xpath>
            </scalar>
            <scalar name="views">
                <xpath>/li[2]/a/div[1]</xpath>
            </scalar>
            <scalar name="downloads">
                <xpath>/li[3]/a/div[1]</xpath>
            </scalar>
            <scalar name="citations">
                <xpath>/li[4]/a/div[1]</xpath>
            </scalar>
            <scalar name="impact_points">
                <xpath>/li[5]/a/div[1]</xpath>
            </scalar>
        </composite>
        <collection name="featured_publications" child_type="research_gate_article" >
            <xpath>//div[@class='c-box js-wexp-publications']/ul/li//h5/a</xpath>
            <scalar name="title">
                <xpath>./span</xpath>
            </scalar> 
            <scalar name="location">
                <xpath>./@href</xpath>
                <regex_op regex="^" replace="/"></regex_op>
            </scalar>                    
        </collection>
        <collection name="top_coauthors" child_type="research_gate_author">
            <xpath>//div[@class='js-coauthor-container']//ul/li//h5/a</xpath>
            <scalar name="title">
                <xpath>.</xpath>
            </scalar>
            <scalar name="location">
                <xpath>./@href</xpath>
                <regex_op regex="^" replace="/"></regex_op>
            </scalar>
        </collection>
        <collection name="skills" child_scalar_type="String">
            <xpath>//div[@class='profile-highlights-skills']//ul/li//a[@class='keyword-list-token-text']</xpath>
        </collection>       
    </meta_metadata>
    
    <meta_metadata name="research_gate_institution" extends="compound_document" parser="xpath">
        <selector url_regex="http://www.researchgate.net/institution/[A-Za-z_]*" domain="researchgate.net"/>
       
        
        <scalar name="institution_head" scalar_type="String">
            <xpath>//div[@class='c-box institution-info']/ul/li[@data-fields='headOfInstitution']</xpath>
        </scalar>
        <collection name="top_publications" child_type="research_gate_article">
            <xpath>//div[@class='institution-publications-content']/ul/li//h5/a</xpath>
            <scalar name="title">
                <xpath>./span</xpath>
            </scalar>
            <scalar name="location">
                <xpath>./@href</xpath>
                <regex_op regex="^" replace="/"></regex_op>
            </scalar>
        </collection> 
        <collection name="top_departments" child_type="research_gate_institution_department">
            <xpath>//div[@class='c-box topDepartments']//div[@class='info']/div/a</xpath>
            <scalar name="title">
                <xpath>.</xpath>
            </scalar>
            <scalar name="location">
                <xpath>./@href</xpath>
                <regex_op regex="^" replace="/"></regex_op>
            </scalar>
        </collection>
        <collection name="top_collaborators" child_type="research_gate_institution">
            <xpath>//ul[@class='institution-list-s']/li//div[@class='name']/a</xpath>
            <scalar name="title">
                <xpath>.</xpath>
            </scalar>
            <scalar name="location">
                <xpath>./@href</xpath>
                <regex_op regex="^" replace="/"></regex_op>
            </scalar>
        </collection>
        <collection name="top_members" child_type="research_gate_author">
            <xpath>//ul[@class='js-people-list-content facepile ']//a/@href</xpath>
            <scalar name="title">
                <xpath>.</xpath>
                <regex_op regex="profile/" replace=""></regex_op>
                <regex_op regex="_" replace=" "></regex_op>
            </scalar>
            <scalar name="location">
                <xpath>.</xpath>
                <regex_op regex="^" replace="/"></regex_op>
            </scalar>
        </collection>  
        <composite name="contact_info" type="research_gate_institution_contact_info">
            <xpath>//div[@class='c-box institution-info']/ul</xpath>
            <scalar name="title">
                <xpath>//div[@class='c-box institution-info']/h4</xpath>
            </scalar>
            <scalar name="telephone" scalar_type="String" schema_org_itemprop="telephone" >
			     <xpath>./li[@data-fields='phone']</xpath>
                <regex_op regex="Phone" replace=""></regex_op>
			</scalar>
			   
			<scalar name="fax_number">
			    <xpath>./li[@data-fields='fax']</xpath>
			    <regex_op regex="Fax" replace=""></regex_op>
			</scalar>
		    <scalar name="street_address">
			     <xpath>./li[@data-fields='street,zip,city,state,country']</xpath>
			     <regex_op regex="Address" replace=""></regex_op>
			</scalar>
        </composite>
        <composite name="website" type="compound_document">
            <xpath>//li[@data-fields='institutionWebsite']</xpath>
           <scalar name="title">
                <xpath>.</xpath>
                <regex_op regex="Website" replace=""></regex_op>
           </scalar>
            <scalar name="location">
                <xpath>.</xpath>
                <regex_op regex="Website" replace="http://"></regex_op>
            </scalar>
        </composite>
           <composite name="stats" type="research_gate_stats">
            <xpath>//div[@class='c-box facility-stats']</xpath>
            <scalar name="publication_count">
                <xpath>./div[2]/div[3]/a/div[1]</xpath>
            </scalar>
            
            <scalar name="impact_points">
                <xpath>./div[2]/div[1]/a/div[1]</xpath>
            </scalar>
            <scalar name="rg_score">
                <xpath>./div[1]/div[1]/a/div[1]</xpath>
            </scalar>
            <scalar name="member_count">
                <xpath>./div[1]/div[3]/a/div[1]</xpath>
            </scalar>
        </composite>
    </meta_metadata>
   
    <meta_metadata name="research_gate_institution_department" extends="compound_document" parser="xpath">
        
        <selector url_regex="http://www.researchgate.net/institution/.*/department/.*" domain="researchgate.net"/>
        <scalar name="title">
            <xpath>//span[@class='fn']</xpath>
        </scalar>
        
        <composite name="stats" type="research_gate_stats">
            <xpath>//div[@class='section-row last with-appendix']</xpath>
            <scalar name="publication_count">
                <xpath>./div[1]/div[1]/a</xpath>
            </scalar>
            
            <scalar name="impact_points">
                <xpath>./div[2]/div[1]</xpath>
            </scalar>
            <scalar name="avg_impact_points">
                <xpath>./div[3]/div[1]</xpath>
            </scalar>
            <scalar name="member_count">
                <xpath>./div[4]/div[1]</xpath>
            </scalar>
        </composite>
        <composite name="institution" type="research_gate_institution">
            <xpath>//div[@class='meta']/a</xpath>
            <scalar name="title">
                <xpath>.</xpath>
            </scalar>
            <scalar name="location">
                <xpath>./@href</xpath>
                 <regex_op regex="^" replace="/"></regex_op>
            </scalar>
        </composite>
        <collection name="top_articles" child_type="research_gate_article">
            <xpath>//div[@class='institution-DepartmentPublicationsContent']/ul/li//h5/a</xpath>
            <scalar name="title">
                <xpath>./span</xpath>
            </scalar>
            <scalar name="location">
                <xpath>./@href</xpath>
                <regex_op regex="^" replace="/"></regex_op>
            </scalar>
        </collection>
        <collection name="top_members" child_type="research_gate_author">
            <xpath>//div[@class='people-list-s institution-members c-box']/ul/li//h5/a</xpath>
            <scalar name="title">
                <xpath>.</xpath>
            </scalar>
            <scalar name="location">
                <xpath>./@href</xpath>
                <regex_op regex="^" replace="/"></regex_op>
            </scalar>
        </collection>
        <collection name="last_weeks_articles" child_type="research_gate_article">
            <xpath>//div[@class='profile-ProfileStatsTopPublicationsList']//div[@class='title']/a</xpath>
            <scalar name="title">
                <xpath>.</xpath>
            </scalar>
            <scalar name="location">
                <xpath>./@href</xpath>
                <regex_op regex="^" replace="/"></regex_op>
            </scalar>
        </collection>
    </meta_metadata>
    
    <meta_metadata name="research_gate_stats" extends="compound_document">
        <scalar name="publication_count" scalar_type="String"></scalar>
        <scalar name="views" scalar_type="String"></scalar>
        <scalar name="downloads" scalar_type="String"></scalar>
        <scalar name="citations" scalar_type="String"></scalar>
        <scalar name="impact_points" scalar_type="String"></scalar>
        <scalar name="avg_impact_points" scalar_type="String"></scalar>
        <scalar name="member_count" scalar_type="String"></scalar>
        <scalar name="rg_score" scalar_type="String"></scalar>
    </meta_metadata>
    <meta_metadata name="research_gate_article_stats" extends="compound_document">
        <scalar name="views" scalar_type="String"/>
        <scalar name="downloads" scalar_type="String"/>
        <scalar name="citations" scalar_type="String"/>
        
    </meta_metadata>
    <meta_metadata name="research_gate_institution_contact_info" extends="contact_point">
        <scalar name="website_address" scalar_type="String"/>
         
    </meta_metadata>
</meta_metadata_repository>