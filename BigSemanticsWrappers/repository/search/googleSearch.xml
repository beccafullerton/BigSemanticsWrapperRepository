    <selector url_regex="https?://www.google.co(m|m?\.\w\w)/search\?.*" domain="google.com">
      <param name="tbm" value_is_not="isch" />
    </selector>
    <selector url_regex="https?://www.google.co(m|m?\.\w\w)/webhp\?.*" domain="google.com">
      <param name="tbm" value_is_not="isch" />
