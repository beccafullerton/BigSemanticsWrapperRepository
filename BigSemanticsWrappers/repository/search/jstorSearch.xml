<meta_metadata_repository name="jstor_search"
  package="ecologylab.bigsemantics.generated.library.jstorSearch">
  
  <meta_metadata name="jstor_search" extends="search" parser="xpath" user_agent_name="chrome_32" comment="jstor Search class">
  		<generic_type_var name="SR" arg="search_result" />
  		<selector url_regex="https?://www.jstor.org/action/doBasicSearch\?.*" domain="jstor.org" />
   		<example_url url="http://www.jstor.org/action/doBasicSearch?Query=internet+everyday"/>
  
  		<scalar name="query">
  			<xpath>//*[@id='searchBox']/@value</xpath>
  		</scalar>
  
  		<scalar name="location" always_show="true"/>
  
  </meta_metadata>

</meta_metadata_repository>