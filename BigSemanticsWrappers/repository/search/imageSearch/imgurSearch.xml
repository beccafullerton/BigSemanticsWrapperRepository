<meta_metadata_repository name="imgurSearch"
  package="ecologylab.bigsemantics.generated.library.imgurSearch" default_user_agent_name="firefox_3_6_8">
  
 

  <meta_metadata name="imgur_search_results" type="image_search" parser="xpath"
    comment="imgur hot search results page" no_cache="true">
    <selector url_stripped="http://imgur.com/search?" />
		<example_url url="http://imgur.com/search?q=league+of+legends"/>
    <collection name="search_results">
      <xpath>//div[@class='post']/a/img</xpath>
      <scalar name="title">
        <xpath>./@src</xpath>
      </scalar>
      <scalar name="location">
        <xpath>./@src</xpath>
      </scalar>
    </collection>
  </meta_metadata>
</meta_metadata_repository>